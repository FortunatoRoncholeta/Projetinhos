program Biblioteca;

uses
  Vcl.Forms,
  Menu in 'Forms\Menu.pas' {F_Menu},
  DM_DADOS in 'Data\DM_DADOS.pas' {DataModule1: TDataModule},
  RenomeiaArquivo in 'Forms\RenomeiaArquivo.pas' {F_RenomearArquivo};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TF_Menu, F_Menu);
  Application.CreateForm(TDataModule1, DataModule1);
  Application.CreateForm(TF_RenomearArquivo, F_RenomearArquivo);
  Application.Run;
end.
